{
  "name": "@alicloud/console-toolkit-plugin-generator",
  "version": "1.0.0-rc.1",
  "description": "console toolkit plugin for generator",
  "main": "lib/index.js",
  "scripts": {
    "ci": "npm run lint && npm run compile && npm run cov",
    "lint": "tslint --project .",
    "compile": "rm -rf ./lib && tsc -d true -p ./",
    "watch": "tsc -watch -d true -p ./",
    "test": "jest",
    "cov": "jest  --coverage",
    "prepare": "npm run compile"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/debug": "^4.1.5",
    "@types/execa": "^2.0.0",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.121",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.7.5",
    "@types/package-json": "^5.0.0",
    "@types/rimraf": "^2.0.2",
    "@types/webpack": "^4.4.27",
    "@types/webpack-chain": "^5.0.1",
    "@types/yeoman-generator": "^3.1.2",
    "chai": "^4.2.0",
    "jest": "^24.2.0-alpha.0",
    "nyc": "^14.1.1",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.8.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@alicloud/console-toolkit-core": "^1.0.0-rc.1",
    "@alicloud/console-toolkit-shared-utils": "^1.0.0-rc.1",
    "chalk": "^2.4.2",
    "execa": "^2.0.4",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "ora": "^3.4.0",
    "package-json": "^6.3.0",
    "rimraf": "^3.0.0",
    "semver": "^6.0.0",
    "user-home": "^2.0.0",
    "yeoman-environment": "^2.3.4"
  },
  "ci": {
    "type": "aoneci",
    "version": "8",
    "docker": true,
    "tags": "",
    "fntest": false,
    "command": "ci",
    "cpp": false,
    "autoBadges": true,
    "image": ""
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "json",
      "json-summary",
      "lcov"
    ],
    "temp-dir": "./node_modules/.nyc_output",
    "sourceMap": true,
    "instrument": true
  },
  "author": "boelroy <boelroy@live.com>",
  "license": "MIT",
  "gitHead": "3fdc9626f6615b1ecaf431fb9267df40b3fc6ef2"
}
