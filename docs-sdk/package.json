{
  "name": "root",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ver": "pnpm recursive --filter !{examples} --filter !root --filter [origin/preset-demos] exec -- npm version",
    "ver:p": "npm run ver -- patch",
    "ver:mi": "npm run ver -- minor",
    "pub": "pnpm publish --filter !{examples} --publish-branch preset-demos",
    "update-deps": "pnpm --recursive update -i -L",
    "sync": "pnpm recursive --filter !{examples} --filter !root --no-sort --workspace-concurrency=Infinity exec -- tnpm sync"
  },
  "devDependencies": {
    "prettier": "^2.2.1"
  }
}
